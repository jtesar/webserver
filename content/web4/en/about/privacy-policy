  <div class="rh-band-header--hash-offset" data-rh-hash="true"id="red-hat-privacy-statement"></div><div class="rh-band-header--component" data-rh-align=left        ><h1 class="rh-band-header-hero-headline" >Red Hat Privacy Statement</h1>
</div>
                                    <!-- Component | Generic -->


    <div class="rh-generic--component"         data-eq-pts="xxs-hr: 0, md-hr: 360, lg-hr: 450"
                 data-rh-align="left"    >
