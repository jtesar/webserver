                    "taxonomySubType": window._rhTarget.util.arrayToString(pageAttributes?.taxonomySubType),
                    "taxonomyChallenge": window._rhTarget.util.arrayToString(pageAttributes?.taxonomyChallenge)
                },
                "success": function(offer) {
                    adobe.target.applyOffer({
                        "mbox": "inline-promo-mbox",
                        "selector": document.querySelector('[data-rh-mbox="inline-promo"]'),
                        "offer": offer
                    });
                },
                // error handling
                "error": function(status, error) {}
