(function($,Drupal,drupalSettings,once){Drupal.behaviors.rhdc_search={attach:function(context,settings){$(once('rhdc_search_submit','form#rhdc_search_form, form[slot="mobile-search"], form[slot="lang-404-search"]',context)).submit(function(e){e.preventDefault();var keys=$(this).find('input[name="search_field"]').val().trim();if(keys)window.location=window.location.origin+'/'+drupalSettings.path.pathPrefix+'search?search='+encodeURIComponent(keys);else window.location=window.location.origin+'/'+drupalSettings.path.pathPrefix+'search';});}};Drupal.behaviors.rhdc_search_autocomplete={attach:function(context,settings){$(once('rhdc_search_suggestions','form#rhdc_search_form',context)).each(function(){suggestionsInit('#rhdc_search_form','.form-wrapper','search_field',false);});$('form[slot="lang-404-search"] input[name="search_field"]').one('keypress',function(){suggestionsInit('form[slot="lang-404-search"]','.form-wrapper','search_field');});function suggestionsInit(formSelector,wrapperSelector,inputName,animate){var $form=$(formSelector);var $wrapper=$form.find(wrapperSelector).first();var $input=$form.find('input[name="'+inputName+'"]');var $container=$wrapper.find('.search-autocomplete__container');var $overlay=$form.next('.search-autocomplete__overlay');var xhr;if(!$overlay.length)$overlay=$('<div>').addClass('search-autocomplete__overlay').insertAfter($form);if(!$container.length)$container=$('<div>').addClass('search-autocomplete__container').appendTo($wrapper);$input.on('keyup',function(e){var keys=$(this).val().trim();var base_url=window.location.origin+'/'+drupalSettings.path.pathPrefix;var url=base_url+'rhdc/jsonapi/suggestion?q='+encodeURIComponent(keys);if(keys&&keys.length>=3&&keys!==$container.data('keys')){$container.data('keys',keys);if(xhr&&xhr.abort)xhr.abort();xhr=$.getJSON(url);xhr.then(function(data){var items=[];if(data&&data.body&&data.body.length){showSuggestions();for(var i=0;i<data.body.length;i++){var row=data.body[i];var $div=$('<div>').addClass('search-autocomplete__item');var href=base_url+'search?search='+encodeURIComponent(row);$('<a>').attr('href',href).text(row).appendTo($div);items.push($div);}$container.empty().append(items);}else $container.data('keys',false);});}}).on('focus',function(e){if($container.data('keys')===$(this).val().trim())showSuggestions(animate);}).on('blur',function(e){if(!$container.hasClass('search-autocomplete-container--active'))hideSuggestions(animate);});$overlay.on('click',function(e){hideSuggestions(animate);});$container.on('mouseenter',function(e){$(this).addClass('search-autocomplete-container--active');}).on('mouseleave',function(e){$(this).removeClass('search-autocomplete-container--active');if(!$input.is(':focus'))hideSuggestions(animate);});function hideSuggestions(animate=true){if(animate){$overlay.fadeOut();$container.slideUp();}}function showSuggestions(animate=true){var $panel=$container.closest('#search-panel');var height=$panel.height();var timeout=$panel.length&&height===0?600:0;setTimeout(function(){if(height||height===0)$panel.data('height',height).height('auto');if(animate){$container.slideDown();$overlay.fadeIn();}},timeout);}}}};})(jQuery,Drupal,drupalSettings,once);;
